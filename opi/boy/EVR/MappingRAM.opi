        <scriptName>Show / hide and update text</scriptName>
        <scriptText><![CDATA[importPackage(Packages.org.csstudio.opibuilder.scriptUtil);

var eventCode = PVUtil.getLong(pvs[0]).longValue();

var visible = true;
var eventName = "";

switch(eventCode) {
case 0x00:
  eventName = "Null Event Code";
  break;
case 0x70:
  eventName = "Seconds '0'";
  break;
case 0x71:
  eventName = "Seconds '1'";
  break;
case 0x7a:
  eventName = "Heartbeat";
  break;
case 0x7b:
  eventName = "Synchronize Prescalers";
  break;
case 0x7c:
  eventName = "Timestamp Counter Increment";
  break;
case 0x7d:
  eventName = "Timestamp Counter Reset";
  break;
case 0x7f:
  eventName = "End of Sequence";
  break;
default:
  visible = false;
  break;
};

widget.setPropertyValue("text", "Special Event: " + eventName);
widget.setPropertyValue("visible", visible);
]]></scriptText>
