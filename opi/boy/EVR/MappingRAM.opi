        <scriptName>Update on PV change</scriptName>
        <scriptText><![CDATA[importPackage(Packages.org.csstudio.opibuilder.scriptUtil);

var pvInternalFunctions = pvs[0];
var pvTrigPulseGens = pvs[1];
var pvSetPulseGens = pvs[2];
var pvResetPulseGens = pvs[3];

var internalFunctionsArray = PVUtil.getLongArray(pvInternalFunctions);
var trigPulseGensArray = PVUtil.getLongArray(pvTrigPulseGens);
var setPulseGensArray = PVUtil.getLongArray(pvSetPulseGens);
var resetPulseGensArray = PVUtil.getLongArray(pvResetPulseGens);

var makeUnsignedLong = function(signedLong) {
  if (signedLong >= 0) {
    return signedLong;
  } else {
    return signedLong + 4294967296;
  }
};

var numberOfEntries = Math.min(internalFunctionsArray.length, trigPulseGensArray.length, setPulseGensArray.length, resetPulseGensArray.length);

var contentArray = Packages.java.lang.reflect.Array.newInstance(Packages.java.lang.reflect.Array.newInstance(Packages.java.lang.String, 0).getClass(), numberOfEntries);
for (var eventCode = 0; eventCode < numberOfEntries; ++eventCode) {
  var entry = Packages.java.lang.reflect.Array.newInstance(Packages.java.lang.String, 6);
  entry[0] = eventCode + "";
  var eventCodeHex = "0x" + Math.floor(eventCode / 16).toString(16) + (eventCode % 16).toString(16);
  entry[1] = eventCodeHex;
  var internalFunctions = makeUnsignedLong(internalFunctionsArray[eventCode]);
  var trigPulseGens = makeUnsignedLong(trigPulseGensArray[eventCode]);
  var setPulseGens = makeUnsignedLong(setPulseGensArray[eventCode]);
  var resetPulseGens = makeUnsignedLong(resetPulseGensArray[eventCode]);
  var internalFunctionsString = "";
  for (var bitIndex = 31; bitIndex >= 0; --bitIndex) {
    if (internalFunctions & (1 << bitIndex)) {
      var bitName;
      switch (bitIndex) {
      case 31:
        bitName = "SEF";
        break;
      case 30:
        bitName = "LTS";
        break;
      case 29:
        bitName = "LED";
        break;
      case 28:
        bitName = "FWD";
        break;
      case 27:
        bitName = "SEL";
        break;
      case 26:
        bitName = "LEV";
        break;
      case 5:
        bitName = "HTB";
        break;
      case 4:
        bitName = "RPS";
        break;
      case 3:
        bitName = "TRE";
        break;
      case 2:
        bitName = "TCE";
        break;
      case 1:
        bitName = "SS1";
        break;
      case 0:
        bitName = "SS0";
        break;
      default:
        bitName = bitIndex + "";
        break;
      }
      if (internalFunctionsString.length > 0) {
        internalFunctionsString += ", ";
      }
      internalFunctionsString += bitName;
    }
  }
  entry[2] = internalFunctionsString;
  var trigPulseGensString = "";
  for (var bitIndex = 0; bitIndex < 32; ++bitIndex) {
    if (trigPulseGens & (1 << bitIndex)) {
      if (trigPulseGensString.length > 0) {
        trigPulseGensString += ", ";
      }
      trigPulseGensString += bitIndex;
    }
  }
  entry[3] = trigPulseGensString;
  var setPulseGensString = "";
  for (var bitIndex = 0; bitIndex < 32; ++bitIndex) {
    if (setPulseGens & (1 << bitIndex)) {
      if (setPulseGensString.length > 0) {
        setPulseGensString += ", ";
      }
      setPulseGensString += bitIndex;
    }
  }
  entry[4] = setPulseGensString;
  var resetPulseGensString = "";
  for (var bitIndex = 0; bitIndex < 32; ++bitIndex) {
    if (resetPulseGens & (1 << bitIndex)) {
      if (resetPulseGensString.length > 0) {
        resetPulseGensString += ", ";
      }
      resetPulseGensString += bitIndex;
    }
  }
  entry[5] = resetPulseGensString;
  contentArray[eventCode] = entry;
}

widget.getTable().setContent(contentArray);
]]></scriptText>
