record(bi, "@PV_PREFIX@EventClock:EvDCM:SRUNFlag") {
  field(DESC, "Event DCM SRUN flag")
  field(INP,  "@PV_PREFIX@Intrnl:EvClockStatus.B3 NPP MS")
