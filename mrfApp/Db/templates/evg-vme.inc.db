  field(ZNAM, "Low")
  field(ONAM, "High")
}

record(bi, "@PV_PREFIX@EventClock:EvDCM:PSInc") {
  field(DESC, "Event DCM phase shift increment")
  field(INP,  "@PV_PREFIX@Intrnl:EvClockStatus.B4 NPP MS")
